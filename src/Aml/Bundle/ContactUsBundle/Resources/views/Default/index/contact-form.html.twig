{# Bloc Contact form #}

<div id="contact-block">
    <h3>Formulaire de contact</h3>

    <form class="form-horizontal fill-up" id="blog-add-content"
          action="{{ path('aml_contact_us_post') }}" method="post" {{ form_enctype(form) }}>

        {{ form_widget(form) }}

        <button class="btn btn-primary pull-right" type="submit">Send</button>

    </form>

</div>