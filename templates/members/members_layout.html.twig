    <!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <title>{% block title %}{% endblock %} {{ site_name }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{{ block('title') }} {{ site_name }}">

    <!--[if lt IE 9]>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js">
    </script>
    <![endif]-->

    <!-- Le styles -->
    <link rel="shortcut icon" href="{{ asset('bundles/amlweb/ico/aml_favicon.ico') }}"/>


    <!-- Latest compiled and minified JavaScript -->
    <link href="{{ asset('build/members-area-style.css') }}" rel="stylesheet"/>

    {% block stylesheets %}{% endblock %}

    {% block headerJs %}{% endblock %}

</head>
<body>
<div id="mdb-preloader" class="flex-center">
    <div id="preloader-markup"></div>
</div>
    <nav class="navbar navbar-dark fixed-top bg-dark flex-md-nowrap p-0 shadow">
        <a class="navbar-brand col-sm-3 col-md-2 mr-0" href="/">{{ site_name }}</a>

        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
        <div id="navbar" class="avbar-nav px-3">
            <ul class="nav navbar-nav navbar-right">
                <li>
                    {% if app.user %}
                    {{ 'layout.welcome_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }}
                    {% endif %}
                </li>
                <li>
                    <a class="nav-link" href="{{ path('app_members_area_logout') }}">
                        {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                    </a>
                </li>
            </ul>
        </div><!--/.navbar-collapse -->
        {% endif %}
    </nav>
    <div class="container-fluid">
        <div class="row">

            <nav class="col-md-2 d-none d-md-block bg-light sidebar">
                <div class="sidebar-sticky">

                    <ul class="nav flex-column">
                        <li class="nav-item"><a class="nav-link" href="#">Agenda</a></li>
                        <li class="nav-item"><a class="nav-link" href="#">Bibliothèque de partitions</a></li>
                        <li class="nav-item"><a class="nav-link" href="#">Documents AML</a></li>
                        <li class="nav-item"><a class="nav-link" href="#">Mes présences</a></li>
                        <li class="nav-item"><a class="nav-link" href="{{ path('aml_users_members_list') }}">Liste des membres</a></li>
                        {% if is_granted('ROLE_SUPER_ADMIN') %}
                        <li class="nav-item"><a class="nav-link" href="{{ path('easyadmin') }}">Admin</a></li>
                        {% endif %}
                    </ul>

                    <h6 class="sidebar-heading d-flex justify-content-between align-items-center px-3 mt-4 mb-1 text-muted">
                        <span>Mes informations</span>
                        <a class="d-flex align-items-center text-muted" href="#">
                            <span data-feather="plus-circle"></span>
                        </a>
                    </h6>

                    <ul class="nav flex-column mb-2">
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path('fos_user_profile_show') }}">Vérifier mes informations</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path('fos_user_profile_edit') }}">Mettre à jour mes informations</a>
                        </li>
                    </ul>
                </div>
            </nav>

            <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">
                <!-- CONTENT HERE -->
                {% for flashMessage in app.session.flashbag.get('success') %}
                    <div class="alert alert-success">
                        {{ flashMessage }}
                    </div>
                {% endfor %}
                {% for flashMessage in app.session.flashbag.get('notice') %}
                    <div class="alert alert-error">
                        {{ flashMessage }}
                    </div>
                {% endfor %}
                {% for flashMessage in app.session.flashbag.get('info') %}
                    <div class="alert alert-info">
                        {{ flashMessage }}
                    </div>
                {% endfor %}
                {% block body %}{% endblock %}
            </main>
        </div>
    </div>

    <script src="{{ asset('build/members-area.js') }}" type="text/javascript"></script>
</body>
</html>
