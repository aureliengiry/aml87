# Cache and logs (Symfony3)
/var/*
!/var/cache
/var/cache/*
!var/cache/.gitkeep
!/var/logs
/var/logs/*
!var/logs/.gitkeep
!/var/sessions
/var/sessions/*
!var/sessions/.gitkeep

# Parameters
/app/config/parameters.yml
/app/config/parameters.ini

# Managed by Composer
/var/bootstrap.php.cache
/bin/*
!bin/console
!bin/symfony_requirements
/vendor/

# Assets and user uploads
/web/bundles/
/web/uploads/
/web/media/
/web/css/

# Assets managed by webpack
/web/build/*
!web/build/.gitkeep

# Build data
/build/

# Composer PHAR
/composer.phar
/composer.lock

# node modules
/node_modules

# Docker
/docker-env
/docker-compose.yml

