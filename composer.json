{
  "name": "aureliengiry/aml87",
  "license": "proprietary",
  "type": "project",
  "description": "AML87 Website",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "classmap": [
      "vendor/google/apiclient/src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.0.0",
    "symfony/swiftmailer-bundle": "^3.2",
    "symfony/monolog-bundle": "^3.2",
    "symfony/dependency-injection": "^3.1",
    "symfony/framework-bundle": "^3.1",
    "symfony/security-bundle": "^3.1",
    "symfony/security-acl": "^3.0",
    "symfony/polyfill-apcu": "^1.6",
    "doctrine/dbal": "^2.5",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.8",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "sensio/framework-extra-bundle": "^5.1",
    "incenteev/composer-parameter-handler": "^2.1",
    "friendsofsymfony/user-bundle": "^2.0",
    "google/apiclient": "^2.1",
    "dms/dms-filter-bundle": "^2.0",
    "sonata-project/doctrine-orm-admin-bundle": "^3.3",
    "sonata-project/intl-bundle": "^2.4",
    "sonata-project/core-bundle": "^3.8",
    "sonata-project/cache": "^1.0",
    "sonata-project/block-bundle": "^3.9",
    "sonata-project/exporter": "^1.8",
    "sonata-project/admin-bundle": "^3.30",
    "jms/serializer": "^1.10",
    "knplabs/knp-menu": "^2.3",
    "knplabs/knp-menu-bundle": "^2.2",
    "twig/twig": "^2.4",
    "sentry/sentry-symfony": "^2.0",
    "symfony/flex": "^1.0",
    "symfony/asset": "^3.1",
    "symfony/orm-pack": "^1.0",
    "symfony/twig-bundle": "^3.1",
    "symfony/form": "^3.1",
    "symfony/translation": "^3.1",
    "symfony/validator": "^3.1"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.0",
    "phpunit/phpunit": "^6.5",
    "nelmio/alice": "^3.1",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "symfony/dotenv": "^3.1",
    "symfony/maker-bundle": "^1.1",
    "symfony/profiler-pack": "^1.0"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "security-checker security:check": "script"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests"
  }
}
