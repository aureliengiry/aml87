{
  "name": "aureliengiry/aml87",
  "license": "proprietary",
  "type": "project",
  "description": "AML87 Website",
  "autoload": {
    "psr-4": {
      "Aml\\": "src/Aml",
      "Tools\\": "src/Tools"
    },
    "classmap": [
      "vendor/google/apiclient/src",
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.5.9",
    "symfony/symfony": "3.3.*",
    "symfony/swiftmailer-bundle": "^2.3.10",
    "symfony/monolog-bundle": "^3.1",
    "symfony/dependency-injection": "^3.1",
    "symfony/framework-bundle": "^3.1",
    "symfony/assetic-bundle": "^2.8",
    "symfony/security-bundle": "^3.0",
    "symfony/security-acl": "^3.0",
    "symfony/polyfill-apcu": "^1.6",
    "doctrine/dbal": "^2.6",
    "doctrine/orm": "^2.6",
    "doctrine/doctrine-bundle": "^1.8",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "^5.1",
    "incenteev/composer-parameter-handler": "^2.1",
    "kriswallsmith/assetic": "^1.4",
    "friendsofsymfony/user-bundle": "^2.0",
    "google/apiclient": "^2.1",
    "dms/dms-filter-bundle": "^2.0",
    "sonata-project/doctrine-orm-admin-bundle": "^3.3",
    "sonata-project/intl-bundle": "^2.4",
    "sonata-project/core-bundle": "^3.8",
    "sonata-project/cache": "^1.0",
    "sonata-project/block-bundle": "^3.9",
    "sonata-project/exporter": "^1.8",
    "sonata-project/admin-bundle": "^3.30",
    "jms/serializer": "^1.10",
    "knplabs/knp-menu": "^2.3",
    "knplabs/knp-menu-bundle": "^2.2",
    "twig/twig": "^2.4",
    "liip/imagine-bundle": "^1.9",
    "egeloen/ckeditor-bundle": "^5.0",
    "sentry/sentry-symfony": "^0.7.1"
  },
  "require-dev": {
    "sensio/generator-bundle": "^3.1",
    "symfony/phpunit-bridge": "^4.0",
    "phpunit/phpunit": "^6.5",
    "nelmio/alice": "^3.1",
    "doctrine/doctrine-fixtures-bundle": "^2.4"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
    ]
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "symlink",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
