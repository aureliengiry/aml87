{
  "name": "aureliengiry/aml87",
  "license": "proprietary",
  "type": "project",
  "description": "AML87 Website",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "classmap": [
      "vendor/google/apiclient/src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "symfony/swiftmailer-bundle": "^3.2",
    "symfony/monolog-bundle": "^3.3",
    "symfony/dependency-injection": "*",
    "symfony/framework-bundle": "*",
    "symfony/security-bundle": "*",
    "symfony/security-acl": "^3.0",
    "symfony/polyfill-apcu": "^1.6",
    "doctrine/dbal": "^2.5",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.8",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "sensio/framework-extra-bundle": "^5.1",
    "incenteev/composer-parameter-handler": "^2.1",
    "google/apiclient": "^2.1",
    "dms/dms-filter-bundle": "^2.0",
    "jms/serializer": "^1.10",
    "knplabs/knp-menu": "^2.3",
    "knplabs/knp-menu-bundle": "^2.2",
    "twig/twig": "^2.4",
    "sentry/sentry-symfony": "^2.0",
    "symfony/flex": "^1.6",
    "symfony/twig-bundle": "*",
    "symfony/form": "*",
    "symfony/translation": "*",
    "symfony/validator": "*",
    "liip/imagine-bundle": "^2.0",
    "friendsofsymfony/ckeditor-bundle": " ^2.0",
    "friendsofsymfony/user-bundle": "^2.1",
    "symfony/templating": "*",
    "doctrine/doctrine-migrations-bundle": "^1.3",
    "symfony/yaml": "*",
    "twig/extensions": "^1.5",
    "easycorp/easyadmin-bundle": "^2.0",
    "symfony/webpack-encore-bundle": "^1.0",
    "symfony/dotenv": "*"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "friendsofphp/php-cs-fixer": "^2.16",
    "nelmio/alice": "^3.5",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^0.12.78",
    "phpstan/phpstan-doctrine": "^0.12.32",
    "phpstan/phpstan-phpunit": "^0.12.17",
    "phpstan/phpstan-symfony": "^0.12.20",
    "phpunit/phpunit": "^7.5",
    "roave/security-advisories": "dev-master",
    "symfony/browser-kit": "4.4.*",
    "symfony/css-selector": "4.4.*",
    "symfony/maker-bundle": "^1.7",
    "symfony/phpunit-bridge": "^6.0",
    "symfony/stopwatch": "4.4.*",
    "symfony/web-profiler-bundle": "4.4.*",
    "symfony/web-server-bundle": "^4.1"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear --no-warmup": "symfony-cmd",
      "cache:warmup": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "phpstan": [
      "@php bin/console cache:warmup --env=dev",
      "@php bin/console cache:warmup --env=test",
      "@phpstan-general",
      "@phpstan-tests"
    ],
    "phpstan-general": "php vendor/bin/phpstan analyse -c phpstan.neon src --level 7 --no-progress",
    "phpstan-tests": "php vendor/bin/phpstan analyse -c phpstan-tests.neon tests --level 7 --no-progress"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "platform": {
      "php": "7.1.99"
    },
    "sort-packages": true
  }
}
